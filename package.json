{
  "name": "@wsgrah/scrolly-lab",
  "private": true,
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "test": "turbo test",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,yml}\"",
    "publish:scrolly": "pnpm --filter @wsgrah/scrolly build && pnpm --filter @wsgrah/scrolly publish --access public",
    "publish:scrolly-three": "pnpm --filter @wsgrah/scrolly-three build && pnpm --filter @wsgrah/scrolly-three publish --access public",
    "publish:all": "pnpm build && pnpm --filter @wsgrah/scrolly publish --access public && pnpm --filter @wsgrah/scrolly-three publish --access public"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json",
    "CHANGELOG.md"
  ],
  "keywords": [],
  "author": "Wayne Graham",
  "license": "MIT",
  "packageManager": "pnpm@10.27.0",
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prettier": "^3.7.4",
    "turbo": "^2.7.2",
    "typescript": "^5.9.3"
  }
}